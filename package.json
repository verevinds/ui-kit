{
  "name": "@verevinds/ui-kit",
  "version": "1.0.28",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "jsnext:main": "dist/index.esm.js",
  "files": [
    "dist/*"
  ],
  "typings": "dist/index.d.ts",
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "eslint": "eslint --debug ./ --fix",
    "eslint:fix": "eslint ./ --fix",
    "prettier": "npx prettier --debug-check ./",
    "prettier:fix": "npx prettier --write ./",
    "stylelint": "stylelint '**/*{.css,.scss}'",
    "stylelint:fix": "stylelint '**/*{.css,.scss}' --fix",
    "build": "cross-env NODE_ENV=production rollup --config",
    "build:watch": "cross-env NODE_ENV=development rollup --config --watch",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "generate": "node ./node_modules/.bin/plop",
    "clean": "rimraf dist && rimraf .cache",
    "tsc": "./node_modules/.bin/tsc"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run prettier:fix",
      "npm run eslint:fix",
      "git add ."
    ],
    "*.{css,scss}": [
      "npm run stylelint:fix",
      "git add ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [],
  "author": "Verevin S. Denis <verevinds@yandex.ru>",
